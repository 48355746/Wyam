Assemblies.AddDirectory(@"..\..\Wyam.Markdown\bin\Debug");
Assemblies.AddDirectory(@"..\..\Wyam.Yaml\bin\Debug");
---
Pipelines.Add(
	ReadFiles("*.md"),
	FrontMatter(Yaml()),
	Markdown(),
	ReplaceIn("{{CONTENT}}", ReadFiles("template.html")),
	Replace("{{TITLE}}", doc => doc.Get("Title", "Default Title")),
	Replace("{{DESC}}", doc => doc.Get("Description", "Default Description")),
	WriteFiles(".html")
);