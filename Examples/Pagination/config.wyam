// Read the template first so we only do it once (but wait to process for each page)
Pipelines.Add("Content",
	ReadFiles("*.md"),
	Markdown(),
	WriteFiles("html")
);

Pipelines.Add("Index",
	ReadFiles("*.cshtml"),
	Paginate(2,
		Documents("Content")		
	),
	Razor(),
	WriteFiles(@doc.Get<int>("CurrentPage") == 1 ? "index.html" : ("index-" + @doc.Get<int>("CurrentPage") + ".html"))
);